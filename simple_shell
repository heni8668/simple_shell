#!/bin/bash

echo "/bin/ls" | ./hsh

echo "ls -l /tmp" | ./hsh

echo "env" | ./hsh

echo "ls /var ; ls /var" |./hsh

echo "ls /hbtn ; ls /var" | ./hsh

echo "ls /var && ls /var && ls /var && ls /hbtn" | ./hsh

echo "ls /var && ls /var && ls /var && ls /hbtn" | ./hsh

echo "echo $?" |./hsh

echo "echo $$" |./hsh

echo "echo $PATH" |./hsh

echo "echo $$ # ls -la" | ./hsh
